<?xml version="1.0" encoding="UTF-8"?>
<connector-plugin class="postgres_jdbc" min-version-tableau="2020.4" name="1" plugin-version="0.0.0" superclass="jdbc" version="18.1">
    <vendor-information>
        <company name="BIGXDATA"/>
        <support-link url="https://bigxdata.io"/>
        <driver-download-link url="https://drivers.example.com"/>
    </vendor-information>
    <connection-customization class="postgres_jdbc" enabled="true" version="10.0">
        <vendor name="vendor"/>
        <driver name="driver"/>
        <customizations>
            <customization name="CAP_FAST_METADATA" value="yes"/>
            <customization name="CAP_JDBC_METADATA_GET_INDEX_INFO" value="no"/>
            <customization name="CAP_JDBC_METADATA_READ_FOREIGNKEYS" value="no"/>
            <customization name="CAP_JDBC_METADATA_READ_PRIMARYKEYS" value="no"/>
            <customization name="CAP_JDBC_METADATA_USE_RESULTSET_FOR_TABLE" value="no"/>
            <customization name="CAP_SELECT_INTO" value="yes"/>
            <customization name="CAP_SELECT_TOP_INTO" value="yes"/>
            <customization name="CAP_CREATE_TEMP_TABLES" value="no"/>
            <customization name="CAP_QUERY_BOOLEXPR_TO_INTEXPR" value="no"/>
